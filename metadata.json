{
  "name": "puppet-report_hipchat",
  "version": "2.0.1-rc0",
  "author": "Vox Pupuli",
  "license": "Apache-2.0",
  "summary": "A Puppet report handler for sending notifications of failed runs to HipChat",
  "source": "https://github.com/voxpupuli/puppet-report_hipchat",
  "project_page": "https://github.com/voxpupuli/puppet-report_hipchat",
  "issues_url": "https://github.com/voxpupuli/puppet-report_hipchat/issues",
  "tags": [
    "hipchat",
    "notifications",
    "report"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "16.04"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/puppetserver_gem",
      "version_requirement": ">= 1.0.0 < 2.0.0"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.10.0 < 6.0.0"
    }
  ]
}
